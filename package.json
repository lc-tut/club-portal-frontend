{
  "name": "club-portal-frontend",
  "version": "0.0.1",
  "description": "Frontend Implementation of 'club-portal'",
  "repository": {
    "type": "git",
    "url": "https://github.com/lc-tut/club-portal-frontend.git"
  },
  "author": "LinuxClub <linuxclub.tut@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env TS_NODE_PROJECT='tsconfig.webpack.json' webpack serve",
    "build": "cross-env TS_NODE_PROJECT='tsconfig.webpack.json' webpack",
    "build:prod": "cross-env TS_NODE_PROJECT='tsconfig.webpack.json' NODE_ENV=production webpack",
    "analyze": "cross-env TS_NODE_PROJECT='tsconfig.webpack.json' NODE_ENV=production webpack --analyze",
    "lint:prettier": "prettier --check \"src/**/*.{js,ts,tsx}\"",
    "lint:eslint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "lint": "run-p lint:prettier lint:eslint",
    "format:prettier": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "format:eslint": "eslint \"src/**/*.{js,ts,tsx}\" --fix",
    "typecheck": "tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@chakra-ui/react": "1.8.6",
    "@emotion/react": "11.8.2",
    "@emotion/styled": "11.8.1",
    "@hookform/resolvers": "2.8.8",
    "axios": "0.26.1",
    "axios-case-converter": "0.9.0",
    "framer-motion": "6.2.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.28.0",
    "react-icons": "4.3.1",
    "react-image-crop": "9.1.1",
    "react-router-dom": "6.2.2",
    "swiper": "8.0.7",
    "swr": "1.2.2",
    "zod": "3.13.4"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@emotion/babel-plugin": "11.7.2",
    "@emotion/babel-preset-css-prop": "11.2.0",
    "@storybook/addon-actions": "6.4.19",
    "@storybook/addon-essentials": "6.4.19",
    "@storybook/addon-links": "6.4.19",
    "@storybook/builder-webpack5": "6.4.19",
    "@storybook/manager-webpack5": "6.4.19",
    "@storybook/react": "6.4.19",
    "@svgr/webpack": "6.2.1",
    "@types/node": "14.18.12",
    "@types/react": "17.0.40",
    "@types/react-dom": "17.0.13",
    "@types/react-image-crop": "8.1.3",
    "@types/webpack-dev-server": "4.7.1",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "babel-loader": "8.2.3",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "css-minimizer-webpack-plugin": "3.4.1",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.3.0",
    "file-loader": "6.2.0",
    "fork-ts-checker-webpack-plugin": "7.2.1",
    "html-webpack-plugin": "5.5.0",
