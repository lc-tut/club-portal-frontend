    "axios": "1.4.0",
    "axios-case-converter": "1.1.0",
    "framer-motion": "10.12.17",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.45.0",
    "react-icons": "4.10.1",
    "react-image-crop": "10.1.5",
