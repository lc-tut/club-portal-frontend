{
  "name": "club-portal-frontend",
  "version": "0.0.1",
  "description": "Frontend Implementation of 'club-portal'",
  "repository": {
    "type": "git",
    "url": "https://github.com/lc-tut/club-portal-frontend.git"
  },
  "author": "LinuxClub <linuxclub.tut@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env TS_NODE_PROJECT='tsconfig.webpack.json' webpack serve",
    "build": "cross-env TS_NODE_PROJECT='tsconfig.webpack.json' webpack",
    "build:prod": "cross-env TS_NODE_PROJECT='tsconfig.webpack.json' NODE_ENV=production webpack",
    "analyze": "cross-env TS_NODE_PROJECT='tsconfig.webpack.json' NODE_ENV=production webpack --analyze",
    "lint:prettier": "prettier --check \"src/**/*.{js,ts,tsx}\"",
    "lint:eslint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "lint": "run-p lint:prettier lint:eslint",
    "format:prettier": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "format:eslint": "eslint \"src/**/*.{js,ts,tsx}\" --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@chakra-ui/react": "1.7.3",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "axios": "0.24.0",
    "axios-case-converter": "0.9.0",
    "framer-motion": "5.5.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.22.1",
    "react-icons": "4.3.1",
    "react-router-dom": "5.3.0",
    "swr": "1.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.16.5",
    "@emotion/babel-plugin": "11.7.1",
    "@emotion/babel-preset-css-prop": "11.2.0",
    "@storybook/addon-actions": "6.4.9",
    "@storybook/addon-essentials": "6.4.9",
    "@storybook/addon-links": "6.4.9",
    "@storybook/react": "6.4.9",
    "@svgr/webpack": "6.1.2",
    "@types/node": "14.18.0",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "5.3.2",
    "@types/webpack-dev-server": "4.5.0",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "babel-loader": "8.2.3",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.4.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "file-loader": "6.2.0",
    "fork-ts-checker-webpack-plugin": "6.5.0",
    "html-webpack-plugin": "5.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "typescript": "4.5.4",
    "webpack": "5.65.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.6.0"
  }
