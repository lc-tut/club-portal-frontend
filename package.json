{
  "name": "club-portal-frontend",
  "version": "0.0.1",
  "description": "Frontend Implementation of 'club-portal'",
  "repository": {
    "type": "git",
    "url": "https://github.com/lc-tut/club-portal-frontend.git"
  },
  "author": "LinuxClub <linuxclub.tut@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env TS_NODE_PROJECT='tsconfig.webpack.json' webpack serve",
    "build": "cross-env TS_NODE_PROJECT='tsconfig.webpack.json' webpack",
    "build:prod": "cross-env TS_NODE_PROJECT='tsconfig.webpack.json' NODE_ENV=production webpack",
    "analyze": "cross-env TS_NODE_PROJECT='tsconfig.webpack.json' NODE_ENV=production webpack --analyze",
    "lint:prettier": "prettier --check \"src/**/*.{js,ts,tsx}\"",
    "lint:eslint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "lint": "run-p lint:prettier lint:eslint",
    "format:prettier": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "format:eslint": "eslint \"src/**/*.{js,ts,tsx}\" --fix",
    "typecheck": "tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@chakra-ui/react": "2.5.2",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@hookform/resolvers": "2.9.11",
    "axios": "1.3.4",
    "axios-case-converter": "1.1.0",
    "framer-motion": "10.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.7",
    "react-icons": "4.8.0",
    "react-image-crop": "10.0.9",
    "react-router-dom": "6.9.0",
    "swiper": "9.1.1",
    "swr": "2.1.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@emotion/babel-plugin": "11.10.6",
    "@emotion/babel-preset-css-prop": "11.10.0",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/builder-webpack5": "6.5.16",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/react": "6.5.16",
    "@svgr/webpack": "6.5.1",
    "@types/node": "18.15.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "babel-loader": "9.1.2",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.7.3",
    "css-minimizer-webpack-plugin": "4.2.2",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "file-loader": "6.2.0",
    "fork-ts-checker-webpack-plugin": "8.0.0",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.7.3",
